            <li><a href="#"> <i class="fa fa-youtube"></i><%= @youtube %></a></li>

          </ul>
        </div>
      </div>
      <div class="col-lg-4 col-md-2 col-sm-2 col-xs-12">
        <div class="footer-widget footer-social">
          <h2 class="widget-title">Endereço da Clínica</h2>
          <ul class="listnone contact">
            <li><i class="fa fa-map-marker"></i><%= @endereco_clinica %></li>
            <li><i class="fa fa-phone"></i> <%= @telefone %></li>
            <li><i class="fa fa-envelope-o"></i> <%= @email %></li>
          </ul>
        </div>
      </div>


    </div>
  </div>


</div>
<div class="tiny-footer">

